<launch>
  <arg name="urdf-name" default="refills_scanner_robot2.xacro"/>
  <arg name="urdf-path" default="$(find refills_scanner_robot_description)/robots/$(arg urdf-name)"/>
  <arg name="param-name" default="robot_description"/>

  <param name="$(arg param-name)" command="$(find xacro)/xacro.py '$(arg urdf-path)'" />
</launch>
